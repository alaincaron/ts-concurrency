{
  "name": "ts-concurrency",
  "version": "1.0.0",
  "description": "Typescript concurrency utilities",
  "main": "./dist/index.js",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "npm run build && mocha -r ts-node/register test/**/*.test.ts",
    "pristine": "rimraf ./dist ./node_modules",
    "build": "rimraf ./dist && tsc",
    "repl": "npm run build && env NODE_PATH=./dist ts-node -i",
    "htmldoc": "typedoc --plugin typedoc-material-theme src/index.ts --out htmldoc",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "coverage": "nyc npm run test",
    "format": "prettier --write .",
    "precommit": "lint-staged",
    "prepare": "husky",
    "benchmark": "npm run build && ./scripts/benchmarks",
    "typedoc": "typedoc --plugin typedoc-plugin-markdown src/index.ts --out docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alaincaron/ts-concurrency.git"
  },
  "keywords": [
    "concurrency",
    "mutex",
    "semaphore",
    "typescript"
  ],
  "author": "Alain Caron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alaincaron/ts-concurrency/issues"
  },
  "homepage": "https://github.com/alaincaron/ts-concurrency#readme",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": "prettier --write",
    "*.js": "prettier --write"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "14.0.0",
    "@types/chai": "5.0.1",
    "@types/mocha": "10.0.10",
    "@types/sinonjs__fake-timers": "8.1.5",
    "@typescript-eslint/eslint-plugin": "8.21.0",
    "benny": "3.7.1",
    "chai": "4.3.10",
    "eslint": "9.19.0",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.2",
    "mocha": "11.1.0",
    "nyc": "17.1.0",
    "prettier": "3.4.2",
    "pretty-quick": "4.0.0",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typedoc": "0.27.6",
    "typedoc-material-theme": "1.3.0",
    "typedoc-plugin-markdown": "4.4.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ts-data-collections": "7.1.3",
    "ts-fluent-iterators": "14.1.3"
  }
}
